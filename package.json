{
  "name": "pallet-mapping-api",
  "version": "1.0.0",
  "main": "./dist/app.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "start": "node ./dist/app.js",
    "startDev": "nodemon ./src/app.ts",
    "local": "SET ENV=local&&yarn startDev",
    "debugStart": "yarn build && supervisor --unhandled-rejections=strict ./dist/app.js",
    "debug": "DEBUG=* && yarn debugStart",
    "debug-mac": "export DEBUG=* && yarn debugStart",
    "dev": "SET ENV=dev&&yarn debugStart",
    "prod": "SET ENV=prod&&yarn debugStart",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "rm -rf dist && tsc",
    "gcp-build": "tsc -p .",
    "deploy": "yarn build && gcloud app deploy",
    "browser": "gcloud app browse",
    "run:dev": "npx functions-framework --target=v2Dev --reload",
    "deployFn:prod": "yarn build && gcloud functions deploy v2 --set-env-vars ENV=prod --runtime nodejs16 --trigger-http --allow-unauthenticated  --region asia-south1",
    "deployFn:dev": "yarn build && gcloud functions deploy v2Dev --set-env-vars ENV=dev --runtime nodejs16 --trigger-http --allow-unauthenticated  --region asia-south1"
  },
  "author": "pallet-mapping-api",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bytes": "^3.1.0",
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "express": "^4.17.1",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^6.14.2",
    "express-winston": "^4.1.0",
    "fcm-notification": "^2.0.0",
    "firebase-admin": "^11.3.0",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "mongodb": "^4.12.1",
    "mongoose": "^6.2.10",
    "multer": "^1.4.3",
    "nodemailer": "^6.6.5",
    "raw-body": "^2.4.1",
    "supervisor": "^0.12.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "^3.1.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.10",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/multer": "^1.4.7",
    "@types/node": "^15.3.0",
    "@types/nodemailer": "^6.4.1",
    "nodemon": "^2.0.22",
    "source-map-support": "^0.5.19",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4"
  }
}
